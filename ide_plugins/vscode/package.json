{
    "name": "omnistack-vscode",
    "displayName": "OmniStack AI",
    "description": "AI-powered development assistance for VS Code",
    "version": "1.0.0",
    "publisher": "omnistack",
    "engines": {
        "vscode": "^1.60.0"
    },
    "categories": [
        "Programming Languages",
        "Snippets",
        "Other"
    ],
    "activationEvents": [
        "onCommand:omnistack.analyzeCode",
        "onCommand:omnistack.debugCode",
        "onLanguage:javascript",
        "onLanguage:typescript",
        "onLanguage:python"
    ],
    "main": "./extension.js",
    "contributes": {
        "commands": [
            {
                "command": "omnistack.analyzeCode",
                "title": "OmniStack: Analyze Code"
            },
            {
                "command": "omnistack.debugCode",
                "title": "OmniStack: Debug Code"
            }
        ]
    },
    "dependencies": {
        "axios": "^1.6.2",
        "vscode": "^1.1.37"
    },
    "devDependencies": {
        "@types/vscode": "^1.60.0",
        "@types/node": "^16.11.7"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/omnistack/omnistack-vscode"
    },
    "keywords": [
        "ai",
        "development",
        "debugging",
        "optimization"
    ],
    "license": "MIT"
}
